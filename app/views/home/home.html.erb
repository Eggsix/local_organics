<div id="home_page">
	<h1>Search For Local Farmers Markets Near You!</h1>
	<div id="area_search">
		<form class="form-inline" action="/search" method="post">
		  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
		  <input class="form-control" type="text" name="search" placeholder="Enter zipcode here">
		  <button class="btn btn-success" type="submit">Submit</button>
		</form>
	</div>
	  <p class='zipcode_error'><%= flash[:errors] %><p>
</div>